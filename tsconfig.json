{
  "compilerOptions": {
    "target": "ES2020",
    "module": "es2020",
    "moduleResolution": "Node",
    "declaration": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "allowUnreachableCode": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "lib": ["ES2020", "ScriptHost", "WebWorker", "WebWorker.ImportScripts"]
  }
}
