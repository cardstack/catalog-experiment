{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "module": "es6",
    "moduleResolution": "Node",
    "pretty": true,
    "target": "ES6",
    "strict": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "allowUnreachableCode": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
